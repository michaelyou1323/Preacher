<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="More"
    >

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp3"
        tools:targetApi="31">
        
        <activity
            android:name="com.michaelyou2000.myapp3.More.ContactUsActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.michaelyou2000.myapp3.More.KerazaActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.michaelyou2000.myapp3.More.EventsActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.michaelyou2000.myapp3.More.Service"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.michaelyou2000.myapp3.New.UserList"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.michaelyou2000.myapp3.HomeActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />

        <activity
            android:name="com.michaelyou2000.myapp3.SplashMenu.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>